<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>CLIENTES & LISTAGEM - Meu app</title>
</head>
<body>
    <h2>Clientes cadastrados:</h2>
    <ul>
        <% lista.forEach(function(user){ %>
            <li><%= user.nome %></li>
            <li><%= user.sobrenome %></li>
            <li><%= user.empresa %></li>
        <% }) %>
    </ul>
</body>
</html>